{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "virtualMachineName": {
            "value": "azure-cse"
        },
        "scriptUris": {
            "value": "https://download.sysinternals.com/files/ProcessExplorer.zip https://raw.githubusercontent.com/gsacavdm/azure-cse/master/windows-cse/examples/upload-storage.ps1"
        },
        "initScriptFileName": {
            "value": "upload-storage.ps1"
        },
        "arguments": {
            "value": "-File ProcessExplorer.zip -StorageAccountName azurecsetest"
        }
    }
}